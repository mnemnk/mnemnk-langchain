{
  "agents": [
    {
      "kind": "Command",
      "name": "mnemnk-langchain-chat-model",
      "title": "Chat Model",
      "category": "LangChain",
      "command": {
        "cmd": "uv",
        "args": ["run", "mnemnk-langchain-chat-model"]
      },
      "inputs": ["*"],
      "outputs": ["message"],
      "default_config": [
        [
          "model",
          {
            "value": "gemma3:4b",
            "type": "string",
            "title": "Chat model"
          }
        ],
        [
          "model_provider",
          {
            "value": "ollama",
            "type": "string",
            "title": "Model provider"
          }
        ]
      ]
    },
    {
      "kind": "Command",
      "name": "mnemnk-langchain-to-human-message",
      "title": "To Human Message",
      "category": "LangChain",
      "command": {
        "cmd": "uv",
        "args": ["run", "mnemnk-langchain-to-human-message"]
      },
      "inputs": ["*"],
      "outputs": ["message"]
    },
    {
      "kind": "Command",
      "name": "mnemnk-langchain-in-memory-chat-history",
      "title": "InMemory Chat History",
      "category": "LangChain",
      "command": {
        "cmd": "uv",
        "args": ["run", "mnemnk-langchain-in-memory-chat-history"]
      },
      "inputs": ["*"],
      "outputs": ["messages"],
      "default_config": [
        [
          "max_messages",
          {
            "value": 5,
            "type": "number",
            "title": "Max messages"
          }
        ]
      ]
    }
  ]
}
