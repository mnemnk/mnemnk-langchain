{
  "agents": [
    {
      "kind": "Command",
      "name": "mnemnk-langchain-chat-model",
      "title": "Chat Model",
      "category": "LangChain",
      "command": {
        "cmd": "uv",
        "args": ["run", "mnemnk-langchain-chat-model"]
      },
      "inputs": ["*"],
      "outputs": ["message"],
      "default_config": [
        [
          "model",
          {
            "value": "gemma3:4b",
            "type": "string",
            "title": "Chat model"
          }
        ],
        [
          "model_provider",
          {
            "value": "ollama",
            "type": "string",
            "title": "Model provider"
          }
        ],
        [
          "kwargs",
          {
            "value": {},
            "type": "object"
          }
        ]
      ]
    },
    {
      "kind": "Command",
      "name": "mnemnk-langchain-to-human-message",
      "title": "To Human Message",
      "category": "LangChain",
      "command": {
        "cmd": "uv",
        "args": ["run", "mnemnk-langchain-to-human-message"]
      },
      "inputs": ["*"],
      "outputs": ["message"]
    },
    {
      "kind": "Command",
      "name": "mnemnk-langchain-to-messages",
      "title": "To Messages",
      "category": "LangChain",
      "command": {
        "cmd": "uv",
        "args": ["run", "mnemnk-langchain-to-messages"]
      },
      "inputs": ["*"],
      "outputs": ["messages"]
    },
    {
      "kind": "Command",
      "name": "mnemnk-langchain-initial-messages",
      "title": "Initial Messages",
      "category": "LangChain",
      "command": {
        "cmd": "uv",
        "args": ["run", "mnemnk-langchain-initial-messages"]
      },
      "inputs": ["reset", "*"],
      "outputs": ["messages"],
      "default_config": [
        [
          "messages",
          {
            "value": null,
            "type": "object",
            "title": "Initial Messages"
          }
        ]
      ]
    },
    {
      "kind": "Command",
      "name": "mnemnk-langchain-recursive-character-text-splitter",
      "title": "Recursive Character Text Splitter",
      "category": "LangChain",
      "command": {
        "cmd": "uv",
        "args": ["run", "mnemnk-langchain-recursive-character-text-splitter"]
      },
      "inputs": ["text"],
      "outputs": ["texts"],
      "default_config": [
        [
          "chunk_size",
          {
            "value": 4000,
            "type": "integer",
            "title": "chunk size"
          }
        ],
        [
          "chunk_overlap",
          {
            "value": 20,
            "type": "integer",
            "title": "chunk overlap"
          }
        ]
      ]
    },
    {
      "kind": "Command",
      "name": "mnemnk-langchain-in-memory-chat-history",
      "title": "InMemory Chat History",
      "category": "LangChain",
      "command": {
        "cmd": "uv",
        "args": ["run", "mnemnk-langchain-in-memory-chat-history"]
      },
      "inputs": ["*"],
      "outputs": ["messages"],
      "default_config": [
        [
          "max_messages",
          {
            "value": 7,
            "type": "number",
            "title": "Max messages"
          }
        ]
      ]
    },
    {
      "kind": "Command",
      "name": "mnemnk-langchain-web-base-loader",
      "title": "Web Base Loader",
      "category": "LangChain",
      "command": {
        "cmd": "uv",
        "args": ["run", "mnemnk-langchain-web-base-loader"]
      },
      "inputs": ["*"],
      "outputs": ["document", "content"]
    },
    {
      "kind": "Command",
      "name": "mnemnk-langchain-youtube-loader",
      "title": "YouTube Loader",
      "category": "LangChain",
      "command": {
        "cmd": "uv",
        "args": ["run", "mnemnk-langchain-youtube-loader"]
      },
      "inputs": ["*"],
      "outputs": ["document", "content"]
    }
  ]
}
